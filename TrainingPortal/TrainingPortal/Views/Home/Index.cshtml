
@{
    ViewBag.Title = "Index";
}

<!-- Page Content -->
<div class="container ">

    <div id="homeIndicator" class="carousel slide my-4" data-ride="carousel">
        <ol class="Home-carousel-indicators Home-new">
            <li data-target="#newestIndicator" data-slide-to="0" class="active"></li>
            <li data-target="#newestIndicator" data-slide-to="1"></li>
            <li data-target="#newestIndicator" data-slide-to="2"></li>
        </ol>


      

        <div class="carousel-inner home-back" role="listbox">

            <div class="carousel-item active wow fadeIn">
                <img src="~/Content/Images/LearnerHomePage/Office-Day@2x.png" />
            </div>

            <div class="carousel-item wow fadeIn">
                <img src="~/Content/Images/EOHMessage.jpg" />
            </div>

            <div class="carousel-item wow fadeIn">

                <img src="~/Content/Images/Updatedsyntxicons.png" />
            </div>

            <div class="home-section">
                <h2 id="Home-Title" class="wow fadeIn" data-wow-delay="0.4s">ANSWER YOUR CALLING</h2>
                <h4 id="home-text" class="wow fadeIn" data-wow-delay="0.5s">Lorem ipsum dolor sit amet, his ad choro scaevola qualisque.</h4>
                <button class="button btn btn-danger wow  zoomIn animated infi" data-wow-duration="0.8s" data-wow-delay="0.1s">@Html.ActionLink("START FOR FREE", "Signup", "Account")</button>
                

            </div>

        </div>


        <!-- END OF -->
        <div id="black-box" style="" class="row">

            <p class="wow fadeInRight" data-wow-delay="1s">MCSD: APP BUILDER</p>
            <p class="wow fadeInRight" data-wow-delay="1.1s">NOW STREAMING</p>
            <p class="wow fadeInRight" data-wow-delay="1.3s">04 - 08 MARCH</p>
        </div>
    </div>
    

    <a class="Home-control-prev" href="#homeIndicator" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="Home-control-next" href="#homeIndicator" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>







</div>--
